<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.auxduty.MainScreen"
    tools:ignore="ExtraText">


    <ImageView android:layout_height="wrap_content"
        android:layout_width="wrap_content"
        android:id="@+id/fireImage">
</ImageView>


    <ImageButton
        android:id="@+id/imageButton4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentRight="true"
        android:layout_marginBottom="83dp"
        android:layout_marginEnd="30dp"
        android:layout_marginRight="30dp"
        android:background="@null"
        android:onClick="joinSessionClicked"
        android:src="@drawable/join_session_drawing"></ImageButton>

    <ImageButton
        android:id="@+id/imageButton5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignTop="@+id/imageButton4"
        android:layout_marginLeft="22dp"
        android:layout_marginStart="22dp"
        android:background="@null"
        android:onClick="startSessionClicked"
        android:src="@drawable/start_session_drawing"></ImageButton>
</RelativeLayout>